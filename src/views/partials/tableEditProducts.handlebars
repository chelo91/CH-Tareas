<table id="tableEditProducts" class="table">
    <thead>
        <tr>
            <th scope="col" data-field="_id">ID</th>
            <th scope="col" data-field="title">Tittle</th>
            <th scope="col" data-field="description">Decription</th>
            <th scope="col" data-field="code">Code</th>
            <th scope="col" data-field="price">Price</th>
            <th scope="col" data-field="status">Status</th>
            <th scope="col" data-field="stock">Stock</th>
            <th scope="col" data-field="category">Category</th>
            <!--<th data-field="thumbnail">Thumbnail</th>-->
        </tr>
    </thead>
    <tbody id="tableEditProductsBody">
        {{#each products}}
        <tr>
            <td>
                <input type="text" name="username" value="{{this._id}}" />
            </td>
            <td>
                <input type="text" name="title" value="{{this.title}}" />
            </td>
            <td>
                <input type="text" name="description" value="{{this.description}}" />
            </td>
            <td>
                <input type="text" name="code" value="{{this.code}}" />
            </td>
            <td>
                <input type="text" name="price" value="{{this.price}}" />
            </td>
            <td>
                <input type="text" name="status" value="{{this.status}}" />
            </td>
            <td>
                <input type="text" name="stock" value="{{this.stock}}" />
            </td>
            <td>
                <input type="text" name="category" value="{{this.category}}" />
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>